<h1>SIGN IN to Chipper: Where Talk is Cheep(s)!</h1>

<%= form_tag sessions_path do %>

  <p>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </p>

  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>

  <p>
    <%= submit_tag "Sign In" %> or <%= link_to "Sign Up", signup_path %>
  </p>

<% end %>
